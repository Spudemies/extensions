{
    "manifest_version": 2,
    "name": "iqdb search",
    "description": "Search by image via iqdb",
    "version": "1.0",
	"icons": {
		"16": "/icons/16x16_iqdb.png",
		"48": "/icons/48x48_iqdb.png"
	},

    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "permissions": [
		"contextMenus",
		"tabs"
    ]
}